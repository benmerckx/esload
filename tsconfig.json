{
  "compilerOptions": {
    "target": "es5",
    "moduleResolution": "node",
    "outDir": "dist",
    "declaration": true,
    "sourceMap": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "downlevelIteration": true
  },
  "include": ["src"]
}
